<head>
    <meta charset="UTF-8">
    <title>출력결과</title>
</head>
<body>
<script>
    let min = 0;

    function findMinNumber(arr){
      
      min = arr[0];
      let length = arr.length;
      
      for (let i = 0; i < length; i++) {
        
        if(arr[i] < min){
          min = arr[i];
        }
        
      }
      
      console.log(min);
   
    }

    let arr=[5, 7, 15, 3, 6, 9, 11];
    findMinNumber(arr);

</script>
</body>
</html>
